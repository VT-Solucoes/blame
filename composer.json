{
  "name": "dbt/blame",
  "license": "MIT",
  "require": {
    "php" : "^8.0.2",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
    "judahnator/trait-aware": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "orchestra/testbench": "^6.0|^7.0",
    "vimeo/psalm": "^4.0"
  },
"autoload": {
    "psr-4": {
        "Dbt\\Blame\\": "src/"
    }
},
  "autoload-dev": {
    "psr-4": {
      "Dbt\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --no-coverage",
    "coverage": "phpunit --coverage-html coverage",
    "stat": "psalm"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Dbt\\Blame\\Provider"
      ]
    }
  }
}
